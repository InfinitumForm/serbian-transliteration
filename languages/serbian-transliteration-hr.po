msgstr "(neobavezno) opis slike za ćirilicu"

#: inc/settings/content/shortcodes.php:53
msgid "(optional) title (alt) description of the image for Latin"
msgstr "(neobavezno) naslov (alt) slike za latinski"

#: inc/settings/content/shortcodes.php:54
msgid "(optional) caption description of the image for Latin"
msgstr "(neobavezno) opis slike za latinski"

#: inc/settings/content/shortcodes.php:55
